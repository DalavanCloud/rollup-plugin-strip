{
  "name": "rollup-plugin-undebug",
  "version": "1.0.0",
  "description": "Remove console, assert and debugger statements from modules",
  "main": "dist/rollup-plugin-undebug.cjs.js",
  "scripts": {
    "test": "mocha",
    "pretest": "npm run build:cjs",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "rollup -c -f cjs -o dist/rollup-plugin-undebug.cjs.js",
    "build:es": "rollup -c -f es6 -o dist/rollup-plugin-undebug.es.js",
    "lint": "eslint src",
    "prepublish": "npm run lint && npm test && npm run build:es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollup/rollup-plugin-undebug.git"
  },
  "keywords": [
    "rollup",
    "modules",
    "debugging",
    "javascript"
  ],
  "author": "Rich Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rollup/rollup-plugin-undebug/issues"
  },
  "homepage": "https://github.com/rollup/rollup-plugin-undebug#readme",
  "dependencies": {
    "acorn": "^3.1.0",
    "magic-string": "^0.15.0",
    "rollup-pluginutils": "^1.3.1"
  },
  "devDependencies": {
    "mocha": "^2.5.3",
    "rollup-plugin-buble": "^0.10.0"
  }
}
