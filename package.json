{
  "name": "rollup-plugin-strip",
  "version": "1.0.1",
  "description": "Remove debugger statements and functions like assert.equal and console.log from your code.",
  "main": "dist/rollup-plugin-strip.cjs.js",
  "scripts": {
    "test": "mocha",
    "pretest": "npm run build:cjs",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "rollup -c -f cjs -o dist/rollup-plugin-strip.cjs.js",
    "build:es": "rollup -c -f es6 -o dist/rollup-plugin-strip.es.js",
    "lint": "eslint src",
    "prepublish": "npm run lint && npm test && npm run build:es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollup/rollup-plugin-strip.git"
  },
  "keywords": [
    "rollup",
    "modules",
    "debugging",
    "javascript"
  ],
  "author": "Rich Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rollup/rollup-plugin-strip/issues"
  },
  "homepage": "https://github.com/rollup/rollup-plugin-strip#readme",
  "dependencies": {
    "acorn": "^3.1.0",
    "magic-string": "^0.15.0",
    "rollup-pluginutils": "^1.3.1"
  },
  "devDependencies": {
    "mocha": "^2.5.3",
    "rollup-plugin-buble": "^0.10.0"
  }
}
